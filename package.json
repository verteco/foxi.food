{
  "name": "foxi-food",
  "version": "1.0.0",
  "description": "Foxi.food - Multi-tenant food ordering platform",
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "dev:frontend": "cd frontend && npm start",
    "dev:backend": "cd backend && python manage.py runserver",
    "build:frontend": "cd frontend && npm run build",
    "start": "echo 'Please use docker-compose up or deploy to Digital Ocean'"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/verteco/foxi.food.git"
  },
  "author": "Verteco",
